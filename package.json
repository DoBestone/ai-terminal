{
  "name": "ai-terminal",
  "version": "1.0.0",
  "description": "SSH终端与AI Agent图形工具",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "NODE_ENV=development concurrently \"npm run watch\" \"electron .\"",
    "watch": "webpack --watch --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "rebuild": "electron-rebuild -f -w node-pty",
    "postinstall": "electron-rebuild -f -w node-pty || true",
    "package": "electron-builder"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@electron/rebuild": "^3.7.2",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "lucide-react": "^0.563.0",
    "node-pty": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ssh2": "^1.14.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  }
